<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Message_Box</title>
<link rel="stylesheet" href="/css/resources/css/0124.css" />
<link rel="stylesheet"
	href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" />
<link rel="stylesheet"
	href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" />
</head>
<body>

	<div class="container">
		<div class="msg-header">
			<div class="msg-header-img">
				<img src="/css/resources/images/0124/man.jpg">
			</div>
			<div class="active">
				<h4>John Lewis</h4>
				<h6>3 hours ago...</h6>
			</div>
			<div class="header-icons">
				<i class="fa fa-phone"></i>
				<i class="fa fa-video-camera"></i>
				<i	class="fa fa-info-circle"></i>
			</div>
		</div>

<div class="chat-page">
<div class="msg-inbox">
		<div class="chats">
		  <div class="msg-page">
		  
				<div class="receved-chats">
				<div class="receved-chats-img">
				<img src="/css/resources/images/0124/man.jpg">
				</div>
				<div class="received-msg">
				<div class="received-msg-inbox">
				<p>Hi !! This is message from John Lewis</p>
				<span class="time">11:01 PM | Ocober 11</span>
				</div>
				</div>
				</div>
		
				<div class="outgoing-chats">
				<div class="outgoing-chats-msg">
				<p>Hi !! This is message from me</p>
				<span class="time">11:01 PM | Ocober 11</span>
				</div>
				<div class="outgoing-chats-img">
				<img src="/css/resources/images/0124/woman.jpg">
				</div>
				</div>
		
				<div class="receved-chats">
				<div class="receved-chats-img">
				<img src="/css/resources/images/0124/man.jpg">
				</div>
				<div class="received-msg">
				<div class="received-msg-inbox">
				<p>Hi !! This is 2nd from John Lewis</p>
				<span class="time">11:01 PM | Ocober 11</span>
				</div>
				</div>
				</div>
		
				<div class="outgoing-chats">
				<div class="outgoing-chats-msg">
				<p>Hi !! This is 2nd from me</p>
				<span class="time">11:01 PM | Ocober 11</span>
				</div>
				<div class="outgoing-chats-img">
				<img src="/css/resources/images/0124/woman.jpg">
				</div>
				</div>
				
				<div class="receved-chats">
				<div class="receved-chats-img">
				<img src="/css/resources/images/0124/man.jpg">
				</div>
				<div class="received-msg">
				<div class="received-msg-inbox">
				<p>Subcribe Easy Tutorials Channel to watch more videos and press bell icon.</p>
				<span class="time">11:01 PM | Ocober 11</span>
				</div>
				</div>
				</div>
		
		</div>
		</div>
		</div>

		<div class="msg-bottom">
				<div class="bottom-icons">
						<i class="fa fa-plus-circle"></i>
						<i class="fa fa-camera"></i>
						<i class="fa fa-microphone"></i>
						<i class="fa fa-smile-o"></i>
				</div>
				<div class="input-group">
					<input type="text" class="form-control" placeholder="write massage...">
						<div class="input-group-append">
							<span class="input-group-text">
								<i class="fa fa-paper-plane"></i>
							</span>
						</div>
				</div>
		</div>		
		
		</div>
		</div>


</body>
</html>